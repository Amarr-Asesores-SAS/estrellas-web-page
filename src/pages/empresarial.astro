---
// Components
// - Layout
import Layout from '../layouts/Layout.astro'
// - UI
import Hero from '../components/blocks/hero/PageHeader.astro'
import CTA from '../components/blocks/CTA/BasicDark.astro'

// Content
// - SEO
const SEO = {
	title: 'EW | Plan Empresarial Estrellas Webcam',
	description:
		'Descubre el mejor plan disponible para que seas una de las mas brillantes estrellas webcam de esta constelación'
}
// - Page Header
const header = {
	title: 'Plan Empresarial <strong>7%</strong>',
	text: 'Nuestra mejor apuesta para tu éxito'
}
// - Testimonial data
import HightlightRows from '@/components/blocks/highlights/HightlightRows.astro'
---

<Layout title={SEO.title} description={SEO.description}>
	<Hero title={header.title} text={header.text} />
	<HightlightRows />
	<CTA />
</Layout>
